{
    "name": "toadgg/lumen-api-starter",
    "description": "A Backbone Framework Integration Lumen Dingoapi Oauth2.",
    "keywords": ["framework", "laravel", "lumen", "oauth2", "dingoapi", "oauth2-server"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "dingo/api": "0.10.*",
        "lucadegasperi/oauth2-server-laravel": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
